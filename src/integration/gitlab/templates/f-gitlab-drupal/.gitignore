# Ignore directories generated by docker
/src/integration/gitlab/templates/f-gitlab-drupal/dev/
!/src/integration/gitlab/templates/f-gitlab-drupal/dev/docker/
# Ignore directories generated by Composer
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/services*.yml
# Ignore Drupal's file directory
/web/sites/*/files
/web/sites/*/private
!/web/sites/default/files/*/sync/*

# Ignore files generated by common IDEs
/.idea/
/.vscode/

# Ignore .env files as they are personal
/.env

# Ignore all files and folder development
/config/dev/*
/config/env.prod

# Ignore downloaded Drupal project.
/drush/
/scripts/
/web/profiles/
/web/sites/

/web/*.*
!/web/autoload.php
.editorconfig
.env.example
.gitattributes
load.*

# Ignore reports, dump and config from this project.
/report-*
/public
/dump/
/local/.env
/local/.local.yml

/tmp*

# Gitlab pages.
/site
node_modules
yarn.lock
package-lock.json
package.json
reports-*
